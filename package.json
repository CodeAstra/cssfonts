{
  "name": "cssicons",
  "version": "0.1.0",
  "description": "Icons using pure CSS",
  "main": "index.js",
  "scripts": {
    "build": "node-sass -w --include-path ./src/scss ./src/scss/main.scss ./public/css/cssicons.css",
    "serve": "lite-server ./public/",
    "start": "concurrently --kill-others \"npm run build\" \"npm run serve\""
  },
  "repository": "git+https://github.com/CodeAstra/cssfonts.git",
  "keywords": [
    "CSS",
    "Icons"
  ],
  "author": "BV Satyaram",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/CodeAstra/cssfonts/issues"
  },
  "homepage": "https://github.com/CodeAstra/cssfonts#readme",
  "devDependencies": {
    "concurrently": "^3.5.1",
    "lite-server": "^2.3.0",
    "node-sass": "^4.9.0",
    "nodemon": "^1.17.5"
  }
}
